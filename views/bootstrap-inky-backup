		script.
			window.jQuery || document.write('<script src="/assets/js/vendor/jquery-slim.min.js"><\\/script>')
		script(src='./Navbar · Bootstrap_files/popper.min.js.Download')
		script(src='./Navbar · Bootstrap_files/bootstrap.min.js.Download')
		script(src='./Navbar · Bootstrap_files/docs.min.js.Download')
		script(src='./Navbar · Bootstrap_files/ie-emulation-modes-warning.js.Download')
		script(src='./Navbar · Bootstrap_files/docsearch.min.js.Download')
		script.
			docsearch({
			apiKey: '48cb48b22351bc71ea5f12f4d1ede198',
			indexName: 'bootstrap-v4',
			inputSelector: '#search-input',
			handleSelected: function (input, event, suggestion) {
			var url = suggestion.url;
			url = suggestion.isLvl1 ? url.split('#')[0]: url;
			// If it's a title we remove the anchor so it does not jump.
			window.location.href = url;
			},
			transformData: function (hits) {
			return hits.map(function (hit) {
			hit.url = hit.url.replace('https://v4-alpha.getbootstrap.com', '/docs/4.0');
			return hit;
			});
			},
			debug: false // Set debug to true if you want to inspect the dropdown
			});
		script.
			Holder.addTheme('gray', {
			bg: '#777',
			fg: 'rgba(255,255,255,.75)',
			font: 'Helvetica',
			fontweight: 'normal'
			});
